def calculate(list_var):
  try:
    if list_var[0] == "*":
      num1 = list_var[1] * list_var[2]
      return num1
    elif list_var[0] == "/":
      num1 = list_var[1] / list_var[2]
      return num1
    elif list_var[0] == "+":
      num1 = list_var[1] + list_var[2]
      return num1
    else:
      num1 = list_var[1] - list_var[2]
      return num1

  except ZeroDivisionError: 
